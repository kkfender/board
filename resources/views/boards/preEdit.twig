{% extends 'layouts/base.twig' %}

{% block title %}Home{% endblock %}

{% block content %}
{% if session('messages') %}
  <div class="alert alert-danger" role="alert">
    {{session('messages')}}
  </div>
{% endif %}

<div class="container mt-4">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <div id="table">
            {{form(form)| raw}}
      </div>
    </div>
  </div>
</div>
{% endblock %}